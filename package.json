{
  "name": "balance-match-test",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:css": "yarn tailwind:watch",
    "dev:server": "vite",
    "build": "run-s build:*",
    "build:css": "yarn tailwind:build",
    "build:app": "tsc && vite build",
    "preview": "vite preview",
    "tailwind:watch": "npx tailwindcss -i ./src/App.css -o ./src/index.css --watch",
    "tailwind:build": "npx tailwindcss -i ./src/App.css -o ./src/index.css --minify"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "npm-run-all": "^4.1.5",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  }
}
