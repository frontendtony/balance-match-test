{
  "name": "balance-match-test",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:css": "yarn tailwind:watch",
    "dev:server": "vite",
    "build": "run-s build:*",
    "build:css": "yarn tailwind:build",
    "dev:test": "cypress open --e2e --browser chrome",
    "build:app": "tsc && vite build",
    "preview": "vite preview",
    "start": "yarn build && vite preview --port 3000",
    "tailwind:watch": "npx tailwindcss -i ./src/App.css -o ./src/index.css --watch",
    "tailwind:build": "npx tailwindcss -i ./src/App.css -o ./src/index.css --minify",
    "test:e2e": "start-server-and-test start http://localhost:3000 cypress:run",
    "cypress:run": "cypress run --browser chrome"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.5",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-query": "^3.39.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "cypress": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.6.3",
    "vite": "^2.9.9",
    "vite-tsconfig-paths": "^3.5.0"
  }
}
